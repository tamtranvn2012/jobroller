#: ../pm4wp.php:156
msgid "1. The plugin goes with a page template for front-end usage."
msgstr "1. Plugin đã có template cho sử dụng ở front-end"

#: ../pm4wp.php:157
msgid "Copy file <code>pm4wp-template.php</code> to your theme folder and create a page with template <code>Private Messages</code>"
msgstr "Copy file <code>pm4wp-template.php</code> vào thư mục của theme và tạo 1 trang mới với template là <code>Private Messages</code>"

#: ../pm4wp.php:158
msgid "The template is just the backbone. You should modify it to fit your theme."
msgstr "Template này chỉ là phần xương sống. Bạn nên chỉnh sửa cho phù hợp với theme của mình."

#: ../pm4wp.php:159
msgid "2. You can send to multiple recipients now."
msgstr "2. Bây giờ bạn có thể gửi tin nhắn cho nhiều người."

#: ../pm4wp.php:161
msgid "Don't show this message next time"
msgstr "Không hiển thị thông báo này trong lần tới"

#: ../pm4wp.php:164
msgid "Please set numbers of private messages for each user role:"
msgstr "Hãy chọn số tin nhắn cho mỗi cấp độ thành viên:"

#: ../pm4wp.php:165
msgid "<b><i>0</i></b> means <b><i>unlimited</i></b>"
msgstr "<b><i>0</i></b> có nghĩa là <b><i>không giới hạn</i></b>"

#: ../pm4wp.php:166
msgid "<b><i>-1</i></b> means <b><i>not allowed</i></b> to send PM"
msgstr "<b><i>-1</i></b> có nghĩa là <b><i>không được phép</i></b> gửi tin nhắn"

