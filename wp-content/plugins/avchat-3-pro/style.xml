<?xml version="1.0" encoding="utf-8"?>
	<styles>
		<backgroundArea>
			<backgroundImageUrl>bg.png</backgroundImageUrl>
			<backgroundImageScale>tile</backgroundImageScale>
			<backgroundImageAlpha>100</backgroundImageAlpha>
		</backgroundArea>
		<fontInputText>
			<fontsize>12</fontsize>
			<fontfamily>_sans</fontfamily>
			<color>#000000</color>
		</fontInputText>
		<textarea>
			<backgroundcolor>#ffffff</backgroundcolor>
			<welcometextcolor>#ff0000</welcometextcolor>
			<historytextcolor>#666666</historytextcolor>
			<opacity>1</opacity>
			<fontsize>12</fontsize>
		</textarea>
		<userlist>
			<backgroundcolor>#ffffff</backgroundcolor>
			<opacity>1</opacity>
		</userlist>
		<istypingbox>
			<backgroundcolor>#ffffcc</backgroundcolor>
			<fontfamily>_sans</fontfamily>
			<color>#ff0000</color>
			<fontsize>12</fontsize>
			<rotationDegrees>-1</rotationDegrees>
		</istypingbox>
		<windows>
			<margin>4</margin>
			<padding>6</padding>
			<backgroundcolor>#123456</backgroundcolor>
			<cornerradius>4</cornerradius>
			<windowsCastShadows>true</windowsCastShadows>
		</windows>
		<fontWindowTitle>
			<fontsize>16</fontsize>
			<fontweight>bold</fontweight>
			<fontfamily>_sans</fontfamily>
			<color>#ffffcc</color>
		</fontWindowTitle>
		<fontStatusBar>
			<fontfamily>_sans</fontfamily>
			<color>#ccd1ff</color>
		</fontStatusBar>
		<fontSideMenuTitle>
			<fontsize>12</fontsize>
			<fontweight>bold</fontweight>
			<fontfamily>_sans</fontfamily>
			<color>#5b9cbe</color>
		</fontSideMenuTitle>
		<fontSideMenuOption>
			<fontsize>12</fontsize>
			<fontweight>normal</fontweight>
			<fontfamily>_sans</fontfamily>
			<color>#d0d0d0</color>
		</fontSideMenuOption>
		<fontSideMenuOptionOver>
			<fontsize>12</fontsize>
			<fontweight>normal</fontweight>
			<fontfamily>_sans</fontfamily>
			<color>#ffffff</color>
		</fontSideMenuOptionOver>
		<fontSideMenuOptionDisabled>
			<fontsize>12</fontsize>
			<fontweight>normal</fontweight>
			<fontfamily>_sans</fontfamily>
			<color>#666666</color>
		</fontSideMenuOptionDisabled>
		<fontSideMenuInfo>
			<fontsize>11</fontsize>
			<fontweight>normal</fontweight>
			<fontfamily>_sans</fontfamily>
			<color>#e63d3d</color>
		</fontSideMenuInfo>
		<fontInputFieldLabel>
			<fontsize>10</fontsize>
			<fontweight>normal</fontweight>
			<fontfamily>_sans</fontfamily>
			<color>#e437fb</color>
		</fontInputFieldLabel>
		<fontMainLabels>
			<fontsize>18</fontsize>
			<fontweight>bold</fontweight>
			<fontstyle>italic</fontstyle>
			<fontfamily>_sans</fontfamily>
			<color>#eb3b2d</color>
		</fontMainLabels>
		<sideMenus>
			<opacity>0.8</opacity>
		</sideMenus>
		<statusBarButtons>
			<fontsize>16</fontsize>
			<fontcolor>#ffffff</fontcolor>
			<backgroundcolor>#5c7796</backgroundcolor>
		</statusBarButtons>
	</styles>